<Window x:Class="WPFClient.AutoesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFClient"
        mc:Ignorable="d"
        Title="AutoesWindow" Height="450" Width="800">
    <Grid>
        <DataGrid HorizontalAlignment="Left" Height="400" Margin="10,10,0,0" Name="autoesDataGridView" VerticalAlignment="Top" Width="774">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Id" Width="50"/>
                <DataGridTextColumn Header="Numar Auto" Width="100"/>
                <DataGridTextColumn Header="Serie Sasiu" Width="100"/>
                <DataGridTextColumn Header="Client" Width="200"/>
                <DataGridTextColumn Header="Sasiu" Width="200"/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
